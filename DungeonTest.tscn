[sub_resource type="GDScript" id="GDScript_nbyfi"]
script/source = "@tool
extends DungeonRoom3D

var pressed_lever_down = false

# Called when the node enters the scene tree for the first time.
func _process(delta):
	var has_character_body = false
	for body in $Area3D.get_overlapping_bodies():
		if body is CharacterBody3D:
			has_character_body = true
	if has_character_body and not pressed_lever_down and Input.is_action_just_pressed(\"interact\"):
		pressed_lever_down = true
		$Models/Lever/lever.rotation.x *= -1
		$TrapFloor.queue_free()
		$Models/floor.queue_free()
"

